{
  "name": "discordjs-v14-snippets",
  "displayName": "DiscordJS V14 Snippets",
  "description": "snippets for discordjs v14",
  "publisher": "Kelopy",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:discordjs-v14-snippets.helloWorld"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "discordjs-v14-snippets.helloWorld",
        "title": "djsv14 snippets"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/embedbuilder.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/buttonbuilder.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
